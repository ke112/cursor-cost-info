{
    "name": "cursor-cost-info",
    "displayName": "Cursor Cost Info",
    "description": "Cursor Cost Info Extension",
    "version": "1.0.81",
    "publisher": "zhangzhihua",
    "author": {
        "name": "徐凤年",
        "email": "zhangzhihua.email@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ke112/cursor-cost-info"
    },
    "homepage": "https://github.com/ke112",
    "bugs": {
        "url": "https://github.com/ke112/cursor-cost-info/issues"
    },
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "cursor.costInfo.showDetails",
                "title": "Show Cursor Cost Details"
            },
            {
                "command": "cursor.costInfo.refresh",
                "title": "Refresh Cursor Cost Info"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "node node_modules/typescript/bin/tsc -p ./",
        "watch": "node node_modules/typescript/bin/tsc -watch -p ./",
        "pretest": "npm run compile",
        "lint": "eslint src --ext ts",
        "package": "vsce package"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.80.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.45.0",
        "typescript": "^5.1.0"
    },
    "dependencies": {
        "@mherod/get-cookie": "^4.2.2"
    }
}